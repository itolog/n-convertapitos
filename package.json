    "@swc/cli": "^0.7.4",
    "@swc/core": "^1.11.24",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.14",
    "@types/supertest": "^6.0.3",
